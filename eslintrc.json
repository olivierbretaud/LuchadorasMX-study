{
  "extends": ["plugin:react/recommended", "airbnb-base"],
  "plugins": ["react"],
  "parser": "babel-eslint",
  "rules": {   
    "no-plusplus": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "react/prop-types": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  },
  "env": {
    "browser": true
  }
} 